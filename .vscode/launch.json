{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cortex-debug",
      "request": "launch",
      "name": "Debug (OpenOCD)",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "runToEntryPoint": "main",
      "executable": "./m20/build/m20.elf",
      "device": "STM32L051R6T6",
      "configFiles": [
        "./m20/openocd/openocd_m20.cfg"
      ],
      "serverArgs": [
        "-s",
        "./m20/openocd/"
      ],
      "postLaunchCommands": [
      "set substitute-path /opt/m20 ./m20"
      ],
      "svdFile": "${workspaceFolder}/svd/STM32L0x1.svd",
      "showDevDebugOutput": "raw"
    }
  ]
}